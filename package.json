{
  "name": "caching-backend-pg",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.13.0"
  },
  "description": "Demo Geocaching coords app based on Postgres",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback --all",
    "db:seed": "knex seed:run",
    "dev": "nodemon src/server",
    "start": "node src/server",
    "test": "cross-env NODE_ENV=test node bin/test.js",
    "test-db:migrate": "cross-env NODE_ENV=test knex migrate:latest",
    "test-db:rollback": "cross-env NODE_ENV=test knex migrate:rollback --all",
    "test-db:seed": "cross-env NODE_ENV=test knex seed:run"
  },
  "keywords": [
    "nodejs",
    "REST",
    "api",
    "express",
    "knex",
    "postgres"
  ],
  "author": "Kaj Lund",
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0"
  },
  "devDependencies": {
    "@japa/assert": "^1.4.1",
    "@japa/runner": "^2.5.1",
    "@japa/spec-reporter": "^1.3.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
